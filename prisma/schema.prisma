generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model quest {
  id                String       @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name_quest        String       @db.VarChar
  quest             String?
  quest_information String?
  Respuestas        Respuestas[]
}

model Respuestas {
  id          String  @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  Respuesta   String
  Id_Pregunta String? @db.Uuid
  quest       quest?  @relation(fields: [Id_Pregunta], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
